{
    "name": "Don't bother me YouTube",
    "description": "Cleanse YouTube's Redirect URLs",
    "version": "1.0.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "all_frames": true,
            "run_at": "document_start",
            "js": [
                "contentscript.js"
            ],
            "matches": [
                "*://*.youtube.com/*"
            ]
        }
    ],
    "icons": {
        "16": "icon16.png",
        "19": "icon19.png",
        "38": "icon38.png",
        "48": "icon48.png",
        "128": "icon128.png",
    }
}
